<html>
<head>
  <title>个人信息</title>
  <meta charset="UTF-8">
<meta name="format-detection" content="telephone=no">
<meta name="viewport" content="width=320,initial-scale=1,maximum-scale=1,minimum-scale=1,user-scalable=no">
<meta http-equiv="pragma" content="no-cache">
<meta http-equiv="cache-control" content="no-cache">
<meta http-equiv="expires" content="0">
<link rel="shortcut icon" href="/favicon.ico">
<link rel="stylesheet" href="/hfive/css/base.css?v=7">
<script type="application/javascript" src="/hfive/lib/fastclick.js"></script>
<script>
  
  if ('addEventListener' in document) {
    document.addEventListener('DOMContentLoaded', function() {
      FastClick.attach(document.body);
    }, false);
  }
  var deviceWidth = document.documentElement.clientWidth;
  if(deviceWidth > 750) deviceWidth = 750;
  document.documentElement.style.fontSize = deviceWidth / 7.5 + 'px';
</script>
  <link rel="stylesheet" href="/hfive/css/profile_edit.css">
</head>
<body>
  <div class="navbar-top"><div class="navbar-content fbox-ac">
    <a class="navbar-left fbox-cc hover" href="javascript:history.go(-1);"><span class="arr arr-left arr-white"></span></a>
    <div class="navbar-center fbox-f1 tc fsize-18">个人信息</div>
  </div></div>
  <div class="container nav fsize-14 full">
    <script type="text/template" id="main-tpl">
    <div class="main">
      <a id="avatar" class="list fbox-ac hover" href="javascript:;">
        <div class="avatar"><img src="${portrait}" onload="resetImg(this)" onerror="setDefaultAvatar(this)"></div><p id="avatar-txt" class="fbox-f1 fsize-12 txt-lv2 tr">{@if portrait}修改{@else}未设置{@/if}</p><span class="arr arr-right arr-gray"></span>
        <input id="avatar-upload" type="file" accept="image/jpg,image/jpeg,image/png,image/bmp,image/gif">
      </a>
      <a id="btn-nickname" class="list fbox-ac hover" href="javascript:;">
        <p class="txt-lv1">姓名</p><p id="nickname-txt" class="fbox-f1 fsize-12 txt-lv2 ellipsis tr">{@if userName}${userName}{@else}未设置{@/if}</p><span class="arr arr-right arr-gray"></span>
      </a>
      <a id="btn-sex" class="list fbox-ac hover" href="javascript:;">
        <p class="txt-lv1">性别</p><p id="sex-txt" class="fbox-f1 fsize-12 txt-lv2 tr">{@if sex === '未设置'}未设置{@else}${sex}{@/if}</p><span class="arr arr-right arr-gray"></span>
      </a>
    </div>
    <a id="btn-logout" class="btn strong hover" href="javascript:;">退出登录</a>
    </script>
  </div>
  <script src="/hfive/lib/zepto.js"></script>
<script src="/hfive/lib/juicer.js"></script>
<script src="/hfive/js/common.js?v=39"></script>
  <script src="http://res.wx.qq.com/open/js/jweixin-1.2.0.js"></script>
  <script src="/hfive/lib/localResizeIMG/lrz.bundle.js"></script>
  <script src="/hfive/js/profile_edit.js?v=1"></script>
</body>
</html>