<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zccbh.demand.mapper.foundation.PosterMapper">
    <select id="findMore" resultType="java.util.Map">
      select p.id,p.title,p.linkTitle,p.linkImg,p.content from cbh_poster p ORDER BY p.createAt desc limit 3
    </select>
</mapper>