#container.ffull {
  min-height: auto; }

.tabbox-tab.null {
  height: 100%; }

.null-info .img-box img {
  width: 1.56rem;
  height: auto; }

.null-info .img-box p {
  color: #d0d0d0; }

.container {
  background-color: #f6f6f6; }

.info {
  margin-top: .1rem;
  background-color: #fff;
  color: #2791ff; }
  .info.top {
    margin-top: 0; }

.i-con {
  display: block;
  margin-right: .2rem;
  width: .6rem;
  height: .6rem;
  background: url(/hfive/img/statistics_sprite_1.png) 0 0/6rem auto no-repeat; }
  .i-con.i4 {
    background-position: -1.8rem 0; }
  .i-con.i5 {
    background-position: -2.4rem 0; }

.title {
  padding: .1rem .3rem;
  border-top: 1px solid #f6f6f6;
  border-bottom: 1px solid #f6f6f6; }
  .title:first-child {
    border-top: none; }

.content {
  padding: .4rem 0; }

.item {
  display: block;
  color: #2791ff;
  border-left: 1px solid #f6f6f6; }
  .item:first-child {
    border-left: none; }
  .item .txt-lv2 {
    margin-top: .2rem; }

#type-box a {
  position: relative;
  color: #666;
  width: 50%;
  height: .8rem;
  border-bottom: 1px solid #fff; }
  #type-box a.active {
    color: #3196fe;
    border-bottom: 1px solid #3196fe; }
  #type-box a:before {
    content: "";
    position: absolute;
    left: 0;
    top: .2rem;
    bottom: .2rem;
    border-left: 1px solid #f6f6f6; }
  #type-box a:first-child:before {
    display: none; }

.list-box {
  padding-bottom: .2rem; }
  .list-box a {
    padding: .2rem .3rem;
    color: #000;
    background-color: #fff;
    border-top: 1px solid #f6f6f6; }
  .list-box .minfo {
    padding: 0 .2rem; }
  .list-box .spec {
    margin: .1rem 0; }
    .list-box .spec .p1 {
      margin: .2rem 1rem 0 0; }
    .list-box .spec .p2 {
      margin: .2rem 0 0; }
  .list-box .arr {
    width: .4rem;
    height: .4rem; }

#top {
  position: fixed;
  top: 0;
  left: 0;
  width: 100%;
  -webkit-transition: -webkit-transform .3s;
  transition: -webkit-transform .3s;
  transition: transform .3s;
  transition: transform .3s, -webkit-transform .3s;
  -webkit-transform: translateY(0) translateZ(0);
  transform: translateY(0) translateZ(0);
  z-index: 990; }
  #top .container {
    padding-top: .2rem; }
