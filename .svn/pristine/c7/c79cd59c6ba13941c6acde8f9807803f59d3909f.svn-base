.container {
  background-color: #f6f6f6; }

.step-box {
  padding: .1rem 0;
  background-color: #fff;
  color: #fff; }
  .step-box .point {
    position: relative;
    width: .8rem;
    height: .8rem;
    margin-left: .7rem;
    background-color: #eaeaea;
    border-radius: 50%; }
    .step-box .point:first-child {
      margin-left: 0; }
      .step-box .point:first-child.active p:before {
        display: none; }
    .step-box .point:last-child:before {
      display: none; }
    .step-box .point:last-child.active p:after {
      display: none; }
    .step-box .point:before {
      content: "";
      position: absolute;
      top: 50%;
      left: 100%;
      margin-left: -.1rem;
      margin-top: -.15rem;
      width: 1rem;
      height: .3rem;
      background-color: #eaeaea; }
    .step-box .point.active p {
      background-color: #50a6ff;
      color: #fff; }
      .step-box .point.active p:before {
        content: "";
        position: absolute;
        top: 50%;
        right: 100%;
        margin-right: -.1rem;
        margin-top: -.075rem;
        width: .6rem;
        height: .15rem;
        background-color: #50a6ff;
        border-top-left-radius: 50px;
        border-bottom-left-radius: 50px;
        z-index: 1; }
      .step-box .point.active p:after {
        content: "";
        position: absolute;
        top: 50%;
        left: 100%;
        margin-left: -.1rem;
        margin-top: -.075rem;
        width: .6rem;
        height: .15rem;
        background-color: #50a6ff;
        border-top-right-radius: 50px;
        border-bottom-right-radius: 50px;
        z-index: 1; }
    .step-box .point p {
      position: absolute;
      top: 50%;
      left: 50%;
      width: .6rem;
      height: .6rem;
      margin: -.3rem 0 0 -.3rem;
      font-size: .2rem;
      color: #484751;
      border-radius: 50%; }
      .step-box .point p span {
        position: relative;
        z-index: 2; }

