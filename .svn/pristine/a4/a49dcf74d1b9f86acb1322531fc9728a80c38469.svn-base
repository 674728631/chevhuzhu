.container{background-color:#f2f5fc}.no-result{position:absolute;top:50%;left:0;width:100%;text-align:center;-webkit-transform:translateY(-50%);transform:translateY(-50%)}.no-result .p1{color:#32373c;margin-bottom:.3rem}.no-result .p2{color:#32373c}#list{padding:.3rem .2rem}.coupon{position:relative;margin-top:.3rem;padding-left:110px;height:120px;background-color:#fff;-webkit-box-shadow:2px 0 10px #999;box-shadow:2px 0 10px #999}.coupon:first-child{margin-top:0}.coupon.used .p1,.coupon.used .p2,.coupon.used .p3{color:#818181}.coupon.used .bot{color:#d9d9d9}.coupon .img{position:absolute;left:-3px;top:0;height:100%}.coupon .p1{margin-bottom:8px;color:#282828}.coupon .p2{padding-right:20px;margin-bottom:15px;color:#a9a9a9}.coupon .p3{color:#282828;text-decoration:line-through;margin-bottom:8px}.coupon .bot{position:absolute;bottom:5px;left:110px;color:#d9d9d9}.coupon .bot img{width:1.2rem;margin:0 4px;vertical-align:middle}.coupon .stamp{position:absolute;height:90px;top:50%;right:10px;margin-top:-45px}.coupon .btn-box{position:absolute;top:50%;right:20px;-webkit-transform:translateY(-50%);transform:translateY(-50%);text-align:center}.coupon .btn-use{display:inline-block;padding:4px 8px;margin-bottom:6px;background-color:#58d6d2;color:#fff;border-radius:15px}.coupon .btn-rule{display:inline-block;color:#58d6d2;text-decoration:underline}