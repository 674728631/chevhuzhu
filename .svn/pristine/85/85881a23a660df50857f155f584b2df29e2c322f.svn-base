<body>
  <div class="navbar-top"><div class="navbar-content fbox-ac">
    <a class="navbar-left fbox-cc hover" href="javascript:history.go(-1);"><span class="arr arr-left arr-white"></span></a>
    <div class="navbar-center fbox-f1 tc fsize-18">案例详情</div>
    <a class="navbar-right fbox-cc hover" href="/hfive/view/index.html"><span class="i-con i-home"></span></a>
  </div></div>
  <div class="container nav-top full fsize-14">
    <script type="text/template" id="main-tpl">
    <div class="tabbox"><div class="tabbox-layer">
      <div class="tabbox-tab">
        <div class="item top">
          <div class="top-item fbox-ac">
            <div class="avatar"><img src="${portrait}" onload="resetImg(this)" onerror="setDefaultAvatar(this)"></div>
            <div class="minfo fbox-f1">
              <p class="fsize-16">{@if nameCarOwner}${nameCarOwner}　{@/if}<span class="txt-blue fsize-14">${licensePlateNumber}</span></p>
              <!--<p class="days t3 fbox-ac"><span class="ic-on i2"></span>加入互助：${joinDay}天</p>-->
              {@if amtAssert}<div class="fbox-ac days t3"><span class="ic-on i3"></span><p class="p1">维修价格：${amtAssert}元</p></div>{@/if}
            </div>
          </div>
        </div>
        <!--<div class="mtitle fsize-20">车辆事故前照片</div>
        <div class="time-box fbox-ac fsize-12"><span class="ic-on i1"></span>{@if examineTime}${examineTime.time|build_time, 'yyyy年MM月dd日'}{@/if}</div>
        <div class="time-item">
          <div class="time-line"></div>
          <div class="item">
            {@if carReasonSuccess}
            <p class="title3 txt-blue">平台审核说明：${carReasonSuccess}</p>
            {@/if}
            {@if carPhotos.length > 0}
            <div class="photo-box fbox wrap no-bdr">
              {@each carPhotos as cpImg}
              <div class="photo-item">
                <a class="photo-img hover" href="javascript:;"><img src="${cpImg}" onload="resetImg(this)" onerror="setDefaultAvatar(this, 'default_photo.png')"></a>
              </div>
              {@/each}
            </div>
            {@/if}
          </div>
        </div>-->
        <div class="mtitle fsize-20">车辆事故照片</div>
        <div class="time-box fbox-ac fsize-12"><span class="ic-on i1"></span>{@if applyTime}${applyTime.time|build_time, 'yyyy年MM月dd日'}{@/if}</div>
        <div class="time-item">
          <div class="time-line"></div>
          <div class="item">
            {@if examineExplanation}
            <p class="title3 txt-blue">平台审核说明：${examineExplanation}</p>
            {@/if}
            <p class="title3" style="padding-top:.1rem;">车主事故描述：${accidentDescription}</p>
            {@if accidentImg.length > 0}
            <div class="photo-box fbox wrap no-bdr">
              {@each accidentImg as cpImg}
              <div class="photo-item">
                <a class="photo-img hover" href="javascript:;"><img src="${cpImg}" onload="resetImg(this)" onerror="setDefaultAvatar(this, 'default_photo.png')"></a>
              </div>
              {@/each}
            </div>
            {@/if}
          </div>
        </div>
        {@if status == 32 || status == 41 || status == 42 || status == 51 || status == 61 || status == 71 || status == 100}
        <div class="mtitle fsize-20">车辆定损</div>
        <div class="time-box fbox-ac fsize-12"><span class="ic-on i1"></span>{@if assertTime}${assertTime.time|build_time, 'yyyy年MM月dd日'}{@/if}</div>
        <div class="time-item">
          <div class="time-line"></div>
          <div class="item">
            <div class="content">
              <div class="info fbox-ac avatar-box">
                <div class="avatar"><img src="{@if assertmanName}${img2}{@else}/hfive/img/manager_6.jpg{@/if}" onload="resetImg(this)" onerror="setDefaultAvatar(this)"></div>
                <p>理赔管家 ${assertmanName}</p>
              </div>
              <div class="info txt single fbox t3">
                <span class="point p1"></span><p class="t1">定损方　</p><p class="fbox-f1">{@if shopName}${shopName}{@else}佳佳{@/if}</p>
              </div>
              <div class="info txt single fbox t3">
                <span class="point p2"></span><p class="t1">订单号　</p><p class="fbox-f1">${orderNo}</p>
              </div>
              <div class="info txt single fbox t3">
                <span class="point p3"></span><p class="t1">定损价格</p><p class="fbox-f1">${amtAssert}元</p>
              </div>
              <div class="info txt single fbox t3">
                <span class="point p4"></span><p class="t1">受损程度</p><p class="fbox-f1">${damageExtent}</p>
              </div>
              <div class="info txt single fbox t3">
                <span class="point p1"></span><p class="t1">受损部位</p><p class="fbox-f1">${damagePosition}</p>
              </div>
              <div class="info txt single fbox t3">
                <span class="point p2"></span><p class="t1">受损描述</p><p class="fbox-f1">${assertDescription}</p>
              </div>
            </div>
            {@if assertImg.length > 0}
            <div class="photo-box fbox wrap">
              {@each assertImg as asImg}
              <div class="photo-item">
                <a class="photo-img hover" href="javascript:;"><img src="${asImg}" onload="resetImg(this)" onerror="setDefaultAvatar(this, 'default_photo.png')"></a>
              </div>
              {@/each}
            </div>
            {@/if}
          </div>
        </div>
        {@/if}
        {@if status == 51 || status == 61 || status == 71 || status == 100}
        <div class="mtitle fsize-20">维修报告</div>
        <div class="time-box fbox-ac fsize-12"><span class="ic-on i1"></span>{@if endRepairTime}${endRepairTime.time|build_time, 'yyyy年MM月dd日'}{@/if}</div>
        <div class="time-item">
          <div class="time-line"></div>
          <div class="item">
            {@if explanationRepair}<p class="title3 t2">${explanationRepair}</p>{@/if}
            {@if repairImg.length > 0}
            <div class="photo-box fbox wrap">
              {@each repairImg as rpImg}
              <div class="photo-item">
                <a class="photo-img hover" href="javascript:;"><img src="${rpImg}" onload="resetImg(this)" onerror="setDefaultAvatar(this, 'default_photo.png')"></a>
              </div>
              {@/each}
            </div>
            {@/if}
          </div>
          {@if status == 61}
          <div class="time-receivecar">
            <div class="receivecar">
              <p>{@if takeCarTime}${takeCarTime.time|build_time, 'yyyy-MM-dd hh:mm:ss'}<br>{@/if}维修厂交车</p>
            </div>
          </div>
          {@/if}
        </div>
        {@/if}
        {@if status == 100}
        <div class="mtitle fsize-20">车主评价</div>
        <div class="time-box fbox-ac fsize-12"><span class="ic-on i1"></span>{@if completeTime}${completeTime.time|build_time, 'yyyy年MM月dd日'}{@/if}</div>
        <div class="time-item">
          <div class="time-line"></div>
          <div class="item">
            <div class="content2">
              <div class="spec fbox-ac">
                <div class="avatar"><img src="${logo}" onload="resetImg(this)" onerror="setDefaultAvatar(this)"></div>
                <p class="fsize-16 t2 p3">${shopName}</p>
                <!--<div class="fbox">
                  <div class="avatar"><img src="${img3}" onload="resetImg(this)" onerror="setDefaultAvatar(this)"></div>
                  <div>
                    <p class="p1 fbox-ac"><span class="ic-on i4"></span>维修技师：${maintenanceshopEmployeename2}</p>
                    <p class="p2 fbox-ac"><span class="ic-on i5"></span>从业年限：${workYear2}年</p>
                  </div>
                </div>-->
              </div>
              <div id="star-box" class="fbox-ac t2">
                <p>服务评分：</p>
                <span class="i-star"></span>
                <span class="i-star{@if commentScore < 2} empty{@/if}"></span>
                <span class="i-star{@if commentScore < 3} empty{@/if}"></span>
                <span class="i-star{@if commentScore < 4} empty{@/if}"></span>
                <span class="i-star{@if commentScore < 5} empty{@/if}"></span>
                <p>　${commentScore}星</p>
              </div>
              <div class="fbox t2"><p>车主评价：</p><p class="fbox-f1">{@if commentContent}${commentContent}{@else}无{@/if}</p></div>
              {@if labelContent}
              <div id="tag-box" class="fbox wrap">
                {@each labelContent as it}
                <a class="active" href="javascript:;">${it}</a>
                {@/each}
              </div>
              {@/if}
            </div>
          </div>
        </div>
        {@/if}
      </div>
    </div></div>
    <!--{@if btn_join.num == 1}
      {@if btn_join.car.status == 1}
      <a id="btn-join" class="btn strong inhover close btn-join" href="/hfive/view/car_add_pay.html?id=${btn_join.car.id}"><p>加入互助</p></a>
      {@else if btn_join.car.status == 2}
      <a id="btn-join" class="btn strong inhover close btn-join" href="/hfive/view/car_add2.html?id=${btn_join.car.id}"><p>加入互助</p></a>
