<html>
<head>
  <title>消息</title>
  <meta charset="UTF-8">
<meta name="format-detection" content="telephone=no">
<meta name="viewport" content="width=320,initial-scale=1,maximum-scale=1,minimum-scale=1,user-scalable=no">
<meta http-equiv="pragma" content="no-cache">
<meta http-equiv="cache-control" content="no-cache">
<meta http-equiv="expires" content="0">
<link rel="shortcut icon" href="/favicon.ico">
<link rel="stylesheet" href="/hfive/css/base.css?v=8">
<script type="application/javascript" src="/hfive/lib/fastclick.js"></script>
<script>
  
  if ('addEventListener' in document) {
    document.addEventListener('DOMContentLoaded', function() {
      FastClick.attach(document.body);
    }, false);
  }
  var deviceWidth = document.documentElement.clientWidth;
  if(deviceWidth > 750) deviceWidth = 750;
  document.documentElement.style.fontSize = deviceWidth / 7.5 + 'px';
</script>
  <link rel="stylesheet" href="/hfive/css/tabbox.css?v=1">
  <link rel="stylesheet" href="/hfive/css/message.css">
</head>
<body>
  <div class="navbar-top"><div class="navbar-content fbox-ac">
    <a class="navbar-left fbox-cc hover" href="javascript:history.go(-1);"><span class="arr arr-left arr-white"></span></a>
    <div class="navbar-center fbox-f1 tc fsize-18">消息</div>
  </div></div>
  <div id="container" class="container nav-top ffull">
  <script type="text/template" id="main-tpl">
    <div id="main">
      <div id="menu-box" class="fbox">
        <a class="fbox-cc fsize-16 txt-lv2 inhover" href="javascript:;" data-value="0"><p>全部</p></a>
        <a class="fbox-cc fsize-16 txt-lv2 inhover" href="javascript:;" data-value="5"><p>我的互助</p></a>
        <a class="fbox-cc fsize-16 txt-lv2 inhover" href="javascript:;" data-value="3"><p>余额变动</p></a>
        <a class="fbox-cc fsize-16 txt-lv2 inhover" href="javascript:;" data-value="1"><p>系统消息</p></a>
      </div>
      <div class="tabbox fsize-14"><div class="tabbox-layer">
        <div class="tabbox-tab">
          <div class="tabbox-refresh fbox-cc fsize-14"><span class="txt">下拉刷新</span><span class="arr3 arr-bottom arr-darkgray"></span></div>
          <ul class="list-box"></ul>
          <div class="null-info tc hide"><div class="img-box"><img src="/hfive/img/icon_noorder.png"><p class="fsize-20">暂无消息</p></div></div>
        </div>
        <div class="tabbox-tab">
          <div class="tabbox-refresh fbox-cc fsize-14"><span class="txt">下拉刷新</span><span class="arr3 arr-bottom arr-darkgray"></span></div>
          <ul class="list-box"></ul>
          <div class="null-info tc hide"><div class="img-box"><img src="/hfive/img/icon_noorder.png"><p class="fsize-20">暂无消息</p></div></div>
        </div>
        <div class="tabbox-tab">
          <div class="tabbox-refresh fbox-cc fsize-14"><span class="txt">下拉刷新</span><span class="arr3 arr-bottom arr-darkgray"></span></div>
          <ul class="list-box"></ul>
          <div class="null-info tc hide"><div class="img-box"><img src="/hfive/img/icon_noorder.png"><p class="fsize-20">暂无消息</p></div></div>
        </div>
        <div class="tabbox-tab">
          <div class="tabbox-refresh fbox-cc fsize-14"><span class="txt">下拉刷新</span><span class="arr3 arr-bottom arr-darkgray"></span></div>
          <ul class="list-box"></ul>
          <div class="null-info tc hide"><div class="img-box"><img src="/hfive/img/icon_noorder.png"><p class="fsize-20">暂无消息</p></div></div>
        </div>
      </div></div>
    </div>
  </script>
  </div>
  <script type="text/template" id="msg-tpl">
    {@each list as it}
    <li><a class="msg-item fbox hover" href="{@if it.type == 3}/hfive/view/wallet_history.html{@else if it.type == 31}/hfive/view/car.html{@else if it.type == 41}/hfive/view/order_detail.html?id=${it.eventNo}{@else}javascript:;{@/if}">
      <span class="i-con i${it.type}"></span>
      <div class="content fbox-f1">
        <div class="fbox-ac wrap"><p class="txt fsize-14 txt-lv2">${it.title}</p><p class="txt2 fsize-12 txt-lv2">${it.createAt.time|build_time, 'yyyy-MM-dd'}</p></div>
        <div class="desc fsize-12 txt-lv1">${it.content}</div>
      </div>
    </a></li>
    {@/each}
  </script>
  <script src="/hfive/lib/zepto.js"></script>
<script src="/hfive/lib/juicer.js"></script>
<script src="/hfive/js/common.js?v=39"></script>
<script>
  if (typeof WeixinJSBridge == "object" && typeof WeixinJSBridge.invoke == "function") {
    handleFontSize();
  } else {
    if (document.addEventListener) {
      document.addEventListener("WeixinJSBridgeReady", handleFontSize, false);
    } else if (document.attachEvent) {
      document.attachEvent("WeixinJSBridgeReady", handleFontSize);
      document.attachEvent("onWeixinJSBridgeReady", handleFontSize);
    }
  }

  function handleFontSize() {
    // 设置网页字体为默认大小
    WeixinJSBridge.invoke('setFontSizeCallback', { 'fontSize' : 0 });
    // 重写设置网页字体大小的事件
    WeixinJSBridge.on('menu:setfont', function() {
      WeixinJSBridge.invoke('setFontSizeCallback', { 'fontSize' : 0 });
    });
  }
</script>
  <script src="http://res.wx.qq.com/open/js/jweixin-1.2.0.js"></script>
  <script src="/hfive/lib/tabbox.js"></script>
  <script src="/hfive/js/message.js?v=2"></script>
</body>
</html>