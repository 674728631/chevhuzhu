<body>
  <div class="navbar-top"><div class="navbar-content fbox-ac">
    <a class="navbar-left fbox-cc hover" href="javascript:history.go(-1);"><span class="arr arr-left arr-white"></span></a>
    <div class="navbar-center fbox-f1 tc fsize-18">互助数据</div>
  </div></div>
  <div class="container nav full fsize-14">
    <script type="text/template" id="main-tpl">
    <div class="info top">
      <div class="title fbox-ac fsize-16"><span class="i-con i1"></span>成员统计</div>
      <div class="content">
        <div class="item">加入总数：<span class="txt-lv2">${userCount}位</span></div>
        <div class="item">截止时间：<span class="txt-lv2">${time|build_time, 'yyyy-MM-dd hh:mm:ss'}</span></div>
      </div>
    </div>
    <div class="info top">
      <div class="title fbox-ac fsize-16"><span class="i-con i2"></span>互助金统计</div>
      <div class="content">
        <div class="item">总额：<span class="txt-lv2">${amtBalance}元</span></div>
        <div class="item">截止时间：<span class="txt-lv2">${time|build_time, 'yyyy-MM-dd hh:mm:ss'}</span></div>
      </div>
    </div>
    <div class="info top">
      <div class="title fbox-ac fsize-16"><span class="i-con i3"></span>事件统计</div>
      <div class="content">
        <div class="item">事件总数：<span class="txt-lv2">${eventCount}起</span></div>
        <div class="item">今日新增数：<span class="txt-lv2">${todayEventCount}起</span></div>
        <div class="item">截止时间：<span class="txt-lv2">${time|build_time, 'yyyy-MM-dd hh:mm:ss'}</span></div>
      </div>
      <div class="bottom">
        <a class="fbox-cc hover btn-action" href="/hfive/view/publicity.html">查看事件</a>
      </div>
    </div>
    </script>
  </div>
  <script src="/hfive/lib/zepto.js"></script>
<script src="/hfive/lib/juicer.js"></script>
<script src="/hfive/js/common.js?v=37"></script>
  <script src="http://res.wx.qq.com/open/js/jweixin-1.2.0.js"></script>
