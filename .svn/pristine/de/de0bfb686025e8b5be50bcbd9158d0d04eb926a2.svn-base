.margin-t-60{
    margin-top: 60px;
}
.margin-t-10{
    margin-top: 10px;
}
.fsize-12{
    font-size: 12px;
}
#desc{
    width: 100%;
    height: 160px;
    outline: none;
    resize: none;
}
.cleanFileBt,.uploadFileBt{
    position: relative;
    margin-left: 20px;
}
.cleanFileBt:hover::after{
    content: '清除';
    height: 21px;
    background-color: orangered;
    color: #ffffff;
    position: absolute;
    top:0;
    right: 0;
    bottom:30px;
    left: 32px;
    width: 42px;
    text-align: center;
    border-radius: 2px;
}
.uploadFileBt:hover::after{
    content: '上传';
    height: 21px;
    background-color: orangered;
    color: #ffffff;
    position: absolute;
    top:0;
    right: 0;
    bottom:30px;
    left: 32px;
    width: 42px;
    text-align: center;
    border-radius: 2px;
}
.tj-tbn{
    float: right;
    margin-top: 30px;
}


/*地图 定位描述*/
.map-address{
    width:100%;height: 30px;
}
#mapContainer{
    border: 1px solid gainsboro;
}
#mapContainerDes{
    position: absolute;
    width: 22%;
    border: 1px solid rgba(220,220,220,0.5);
    height: 85%;
    top:80px;
    right: 0;
    overflow-y: scroll;
}
.map-des{
    cursor: pointer;
    width: 100%;
    min-height: 90px;
    padding-left: 20px;
    overflow: hidden;
}
.map-des:hover{
    background-color: gainsboro;
}
.map-des h3{
    font-size: 14px;
    font-weight: bold;
    margin-top: 10px;

}
.map-address i{
    font-style: normal;
}

.map-des span{
    display: inline-block;
    margin: 5px 0;
}
.base-title{
    margin: 10px 0;
}

/*新增 轮播 上传的车照片*/

html, body {
    position: relative;
    height: 100%;
}
#swiperLay .xs-title{
    height: 46px;
    line-height:46px;
    text-indent: 20px;
    font-weight: bold;
    text-align: right;
    position: relative;
    z-index: 999;
}
.swiper-container {
    /*width: 624px;*/
    max-width: 100%;
    height: 96%;
    margin: 0 0 4%;
    display: none;
}
#swiperSon .swiper-slide{
    /*overflow: scroll;*/
    background-color: #FFFFFF;
}
#swiperSon .swiper-slide img{
    width: 100%;
    padding-top: 46px;
}

/*理赔列表 新增搜索框*/
.new-search-btn{
    /*margin: 30px 0 20px;*/
}
.margin-row{
    margin: 10px 0 20px
}
.margin-row-max{
    margin: 60px 0 30px;
}
.new-search-btn .form-control{
    width: 80%;
    border-radius:0;
    border-top-left-radius: 4px;
    border-bottom-left-radius: 4px;
}
.new-search-btn .def-btn{
    width: 18%;
}
.new-search-btn .btn{
    border-radius: 0;
    outline: none;
}
.new-search-btn input,.new-search-btn button{
    float: left;
}
/*旋转按钮*/
.rote-btn{
    font-size: 16px;
    margin-right: 20px;
    cursor: pointer;
}
.rote-btn:hover{
    transform: scale(1.2);
    transition: 0.3s all linear;
}
/*推广奖励 列表*/
.tk-search-box{
    padding-left: 20px;
    margin: 20px 0 5px;
    float: left;
}

.tk-mav-tabs .nav-tabs{
    border-bottom: 1px solid #dddddd;
}

.time-select-box label,.time-select-box input{
    float: left;
}
.time-select-box label{
    margin: 6px 2%;
    color: grey;
}

.time-select-box input{
    width:15% !important;
}
.end-time-sel{
    border-top-right-radius: 4px !important;
    border-bottom-right-radius: 4px !important;
}
.time-select-box button{
    margin-left: 5%;
}
.add-award-table{
    margin-top: 20px;
}
.add-award-table,
.add-award-table > thead > tr > th,
.add-award-table > tbody > tr > th,
.add-award-table > tfoot > tr > th,
.add-award-table > thead > tr > td,
.add-award-table > tbody > tr > td,
.add-award-table > tfoot > tr > td{
    border: none;
}
.add-award-table > tbody > tr > td{
    min-height: 34px;
    line-height: 34px;
}
.add-award-table tr td{
    text-align: right;
}
textarea{
    resize: none;
}

.new-ad-box{
    margin: 60px 20px 20px;
    background-color: #fff;
}
.def-table > thead > tr > th, .def-table > tbody > tr > th, .def-table > tfoot > tr > th, .def-table > thead > tr > td, .def-table > tbody > tr > td, .def-table > tfoot > tr > td {
    line-height: 30px;
    text-align: center;
}
.jl-tab-content{
