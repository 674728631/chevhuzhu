.kkpager{
	clear:both;
	height:30px;
	line-height:30px;
	margin-top:10px;
	color:#999999;
	font-size:12px;
	
}
.kkpager a{
	padding:4px 8px;
	margin:10px 3px;
	font-size:12px;
	border:1px solid #DFDFDF;
	background-color:#FFF;
	color:#1d6fa5;
	text-decoration:none;
	display:inline-block;
	padding:0 10px;
	
}
.kkpager span{
	font-size:14px;
	display:inline-block;
	padding:0 10px;
}
.kkpager span.disabled{
	/* padding:4px 8px; */
	margin:10px 3px;
	font-size:12px;
	border:1px solid #DFDFDF;
	background-color:#FFF;
	color:#DFDFDF;
}
.kkpager span.curr{
	/* padding:4px 8px; */
	margin:10px 3px;
	font-size:12px;
	border:1px solid #3498db;
	background-color:#3498db;
	color:#FFF;
}
.kkpager a:hover{
	background-color:#3498db;
	border:1px solid #3498db;
	color:#fff;
}
.kkpager span.normalsize{
	font-size:12px;
	display:none;
}
.kkpager_gopage_wrap{
	display:inline-block;
	width:40px;
	height:21px;
	border:1px solid #DFDFDF;
	margin:0px 1px;
	padding:0px;
	position:relative;
	left:0px;
	top:7px;
}
.kkpager_btn_go {
	width:44px;
	height:21px;
	line-height:21px;
	padding:0px;
	font-family:arial,����,sans-serif;
	text-align:center;
	border:0px;
	background-color:#3498db;
	color:#FFF;
	position:absolute;
	top:-1px;
	display:none;
	font-size:12px;
}
.kkpager_btn_go_input{
	width:38px;
	height:19px;
	text-align:center;
	border:0px;
	position:absolute;
	left:0px;
	top:0px;
	outline:none;
}