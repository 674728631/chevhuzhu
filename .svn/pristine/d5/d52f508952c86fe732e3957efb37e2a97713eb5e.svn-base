    {@if sid == 161}
      <img src="/hfive/img/event_join_1.jpg?1" width="100%">
      <img id="img2" src="/hfive/img/event_join_3.png">
      <input id="phone" class="fsize-16 fstrong v2" type="tel" placeholder="请输入手机号码" maxlength="11">
      <a id="btn-confirm" class="fbox-cc fsize-16 hover v2" href="javascript:;">立即领取</a>
    {@else}
      <img src="/hfive/img/event_join_1.jpg?1" width="100%">
      <img id="img2" src="/hfive/img/event_join_4.png">
      <img id="logo" src="${data.logo}" style="width:${data.width};left:{@if data.left}${data.left}{@else}1rem{@/if};top:{@if data.top}${data.top}{@else}1.2rem{@/if};">
      <input id="phone" class="fsize-16 fstrong v2" type="tel" placeholder="请输入手机号码" maxlength="11">
